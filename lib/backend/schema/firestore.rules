rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /telkes/{document} {
      allow create: if request.auth.uid == request.resource.data.uid;
      allow read: if true;
      allow write: if true;
      allow delete: if true;
    }

    match /Suhu/{document} {
      allow create: if true;
      allow read: if true;
      allow write: if true;
      allow delete: if true;
    }
  }
}
